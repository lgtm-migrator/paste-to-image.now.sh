{
  "compilerOptions": {
    "jsx": "react",
    "target": "ES2019",
    "module": "commonjs",
    "lib": ["es6", "dom"],
    "strict": false,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "declaration": false,
    "noEmit": true,
    "esModuleInterop": true
  },
  "exclude": ["node_modules", "dist"],
  "include": ["src"]
}
